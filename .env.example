# Temporary Role ID to assign during script activation
ROLE_TEMP=REPLACE_WITH_TEMP_ROLE_ID

# Channel IDs for logging, script delivery, and deny messages
LOG_CHANNEL=REPLACE_WITH_LOG_CHANNEL_ID
SCRIPT_CHANNEL=REPLACE_WITH_SCRIPT_CHANNEL_ID
DENY_CHANNEL=REPLACE_WITH_DENY_CHANNEL_ID